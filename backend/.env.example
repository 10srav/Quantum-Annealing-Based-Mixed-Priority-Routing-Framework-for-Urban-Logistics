# API settings
API_HOST=0.0.0.0
API_PORT=8000
CORS_ORIGINS=["http://localhost:5173","http://localhost:3000"]

# Authentication
# Generate hash with: python -c "import hashlib; print(hashlib.sha256(b'your-api-key').hexdigest())"
